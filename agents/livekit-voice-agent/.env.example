# LiveKit Configuration
LIVEKIT_URL=wss://plug-bq7kgzpt.livekit.cloud
LIVEKIT_API_KEY=your-livekit-api-key
LIVEKIT_API_SECRET=your-livekit-api-secret

# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Supabase Storage S3 Credentials (for LiveKit recording)
# Generate at: https://supabase.com/dashboard/project/YOUR_PROJECT/storage/settings
SUPABASE_S3_ACCESS_KEY=your_s3_access_key
SUPABASE_S3_SECRET_KEY=your_s3_secret_key
SUPABASE_S3_ENDPOINT=https://YOUR_PROJECT.supabase.co/storage/v1/s3
SUPABASE_S3_REGION=us-east-1

# SignalWire Configuration (for telephony/call transfer)
SIGNALWIRE_SPACE=your_signalwire_space.signalwire.com
SIGNALWIRE_PROJECT_ID=your_project_id
SIGNALWIRE_API_TOKEN=your_api_token

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key

# Deepgram Configuration (for STT)
DEEPGRAM_API_KEY=your_deepgram_api_key

# ElevenLabs Configuration (for TTS and voice cloning)
ELEVENLABS_API_KEY=your_elevenlabs_api_key

# Local Testing (optional)
# Set a different agent name for local testing to avoid conflicts with Render
# Requires separate LiveKit dispatch rule for test number
# LIVEKIT_AGENT_NAME=SW Telephony Agent Local
